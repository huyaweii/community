page {
  background: #efeded;
}
.container {
  margin: 30rpx 40rpx 0rpx 40rpx;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
}
.tabContent{
  width: 20rpx;
  color: #f00;

  font-size: 12rpx;
}
.pictureOuter {
  width: 100%;
  background: #fff;
  padding-bottom: 20rpx;
}
.pictureWrap {
  width: 740rpx;
  margin: 0 auto;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.formItem image {
  width: 180rpx;
  height: 180rpx;
  margin: 0rpx 2.5rpx 5rpx 2.5rpx;
}
.iconWrapper {
  width: 180rpx;
  height: 180rpx;
  text-align: center;
  line-height: 180rpx;
  background: rgb(224, 224, 224);
  margin: 10rpx 2.5rpx 0 2.5rpx;
}
.iconWrapper i {
  color: #fff;
  font-size: 80rpx;
}
.formItem {
  width: 100%;
  font-size: 32rpx;
  color: #666;
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 20rpx;
}
.formItem .title {
  width: 120rpx;
}
.formItem input {
  border: 1px solid #bdb9b9;
  flex: 1;
  padding-left: 10rpx;
}
.formPic {
  display: flex;
  flex-direction: row;
  align-items: flex-start;
}
.submit {
  position: fixed;
  left: 0rpx;
  bottom: 0rpx;
  background: #fd790d;
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
  width: 100%;
  color: #fff;
}